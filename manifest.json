{
  "manifest_version": 3,
  "name": "Clean Data - Bookmarks Whitelist",
  "version": "1.1",
  "description": "Supprime toutes les donn√©es sauf celles des bookmarks",
  "default_locale": "fr",
  "permissions": [
    "bookmarks",
    "browsingData",
    "notifications",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
