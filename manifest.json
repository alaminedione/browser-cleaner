{
  "manifest_version": 3,
  "name": "Clean Data Except Bookmarks",
  "version": "1.0",
  "description": "Supprime les données des sites non bookmarqués",
  "permissions": ["bookmarks", "browsingData"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
